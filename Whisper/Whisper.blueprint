[BLUEPRINT] v1
[NAME] Whisper
[TYPE] Prefab
[VERSION] latest
[ENVIRONMENT] any

[DEPEND]
- [BASE] python [any]
- [APT] ffmpeg [any]
- [PYTHON] numba [any]
- [PYTHON] numpy [any]
- [PYTHON] torch [any]
- [PYTHON] tqdm [any]
- [PYTHON] more-itertools [any]
- [PYTHON] tiktoken [any]
- [PYTHON] triton [>=2.0.0]
- [LOCAL] https://github.com/ultralytics/yolov5 [/usr/src/app]

[WORKDIR] /
[CMD]
- whisper --model_dir /usr/local --model medium audio >> /dev/null 2>&1; exit 0
